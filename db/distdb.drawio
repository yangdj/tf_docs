                <mxCell id="hL-x_0z0JpgMXZ3rjriP-0" value="&lt;table cellpadding=&quot;4&quot; cellspacing=&quot;0&quot; border=&quot;1&quot; style=&quot;font-size: 1em ; width: 100% ; height: 100%&quot;&gt;&lt;tbody&gt;&lt;tr&gt;&lt;th&gt;application&lt;/th&gt;&lt;th&gt;install &amp;amp; envs&lt;/th&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;scala&lt;/td&gt;&lt;td&gt;&lt;h3&gt;install&lt;/h3&gt;&lt;div&gt;&lt;font face=&quot;Courier New&quot;&gt;SCALA_VERSION=2.12.8&lt;br&gt;&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face=&quot;Courier New&quot;&gt;wget https://downloads.lightbend.com/scala/${SCALA_VERSION}/scala-${SCALA_VERSION}.tgz&lt;/font&gt;&lt;br&gt;&lt;/div&gt;&lt;h3&gt;envs&lt;/h3&gt;&lt;div&gt;&lt;div&gt;&lt;font face=&quot;Courier New&quot;&gt;export SCALA_HOME=/server/proj/hp/bin/scala/scala-2.12.8&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face=&quot;Courier New&quot;&gt;export PATH=${SCALA_HOME}/bin:${PATH}&lt;/font&gt;&lt;/div&gt;&lt;/div&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;maven&lt;br&gt;&lt;/td&gt;&lt;td&gt;&lt;h3&gt;&lt;span style=&quot;text-align: center&quot;&gt;install&lt;/span&gt;&lt;/h3&gt;&lt;font face=&quot;Courier New&quot;&gt;&lt;span style=&quot;text-align: center&quot;&gt;MAVEN_VERSION=3.5.4&lt;/span&gt;&lt;br&gt;&lt;/font&gt;&lt;div&gt;&lt;font face=&quot;Courier New&quot;&gt;wget&amp;nbsp;&lt;/font&gt;&lt;a href=&quot;http://mirrors.tuna.tsinghua.edu.cn/apache/maven/maven-3/$%7BMAVEN_VERSION%7D/binaries/apache-maven-$%7BMAVEN_VERSION%7D-bin.tar.gz&quot;&gt;http://mirrors.tuna.tsinghua.edu.cn/apache/maven/maven-3/${maven_version}/binaries/apache-maven-${maven_version}-bin.tar.gz&lt;/a&gt;&lt;/div&gt;&lt;div style=&quot;font-family: &amp;#34;courier new&amp;#34;&quot;&gt;tar xf apache-maven-${MAVEN_VERSION}-bin.tar.gz&lt;/div&gt;&lt;div style=&quot;font-family: &amp;#34;courier new&amp;#34;&quot;&gt;&lt;br&gt;&lt;/div&gt;&lt;h3&gt;envs&lt;/h3&gt;&lt;div&gt;&lt;p&gt;&lt;span style=&quot;font-family: &amp;#34;courier new&amp;#34; ; font-size: 1em ; font-weight: normal&quot;&gt;export M2_HOME=/server/proj/hp/bin/maven/apache-maven-3.6.1&lt;br&gt;&lt;/span&gt;&lt;font face=&quot;Courier New&quot;&gt;export PATH=${M2_HOME}/bin:${PATH}&lt;br&gt;&lt;/font&gt;[&lt;font face=&quot;Courier New&quot; style=&quot;font-size: 1em ; font-weight: normal&quot;&gt;export MAVEN_OPTS=&quot;-Xmx2g -XX:ReservedCodeCacheSize=1024m&quot;&lt;/font&gt;&lt;span style=&quot;font-size: 1em ; font-weight: normal&quot;&gt;&amp;nbsp;&lt;/span&gt;&lt;span style=&quot;font-size: 1em ; font-weight: normal&quot;&gt;# for java8+]&lt;/span&gt;&lt;/p&gt;&lt;/div&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;spark&lt;/td&gt;&lt;td&gt;&lt;h3&gt;install&lt;/h3&gt;&lt;div&gt;&lt;font face=&quot;Courier New&quot;&gt;SPARK_VERSION=2.4.1&lt;br&gt;&lt;/font&gt;&lt;/div&gt;&lt;h4&gt;1. binary file&lt;/h4&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;h3&gt;2. from source&lt;/h3&gt;&lt;div&gt;&lt;font face=&quot;Courier New&quot;&gt;// since spark 2.3.0&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face=&quot;Courier New&quot;&gt;wget https://archive.apache.org/dist/spark/spark-${SPARK_VERSION}/spark-${SPARK_VERSION}.tgz&lt;br&gt;&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-family: &amp;#34;courier new&amp;#34; ; font-size: 1em&quot;&gt;&lt;br&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-family: &amp;#34;courier new&amp;#34; ; font-size: 1em&quot;&gt;//&amp;nbsp;&lt;/span&gt;&lt;span&gt;&lt;font face=&quot;courier new&quot;&gt;maybe wait long to compile successfully, and may fail for network cause, just recompile it&lt;/font&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-family: &amp;#34;courier new&amp;#34; ; font-size: 1em&quot;&gt;./dev/make-distribution.sh --name &quot;hadoop2-without-hive&quot; --tgz \&lt;/span&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;font face=&quot;Courier New&quot;&gt;&quot;-Pyarn,hadoop-provided,hadoop-2.7,parquet-provided,orc-provided&quot;&lt;br&gt;&lt;/font&gt;&lt;/div&gt