uot;&gt;.groupByKey().count()&lt;/font&gt;&lt;/li&gt;&lt;/ol&gt;&lt;/td&gt;&lt;td&gt;&lt;ol&gt;&lt;li&gt;&lt;span style=&quot;font-family: &amp;#34;courier new&amp;#34; ; font-size: 1em&quot;&gt;.count()&lt;/span&gt;&lt;br&gt;&lt;/li&gt;&lt;li&gt;&lt;font face=&quot;Courier New&quot;&gt;.first()&lt;/font&gt;&lt;/li&gt;&lt;li&gt;&lt;font face=&quot;Courier New&quot;&gt;.take(10)&lt;/font&gt;&lt;/li&gt;&lt;li&gt;&lt;font face=&quot;Courier New&quot;&gt;.collect()&lt;/font&gt;&lt;/li&gt;&lt;li&gt;&lt;font face=&quot;Courier New&quot;&gt;.reduce()&lt;/font&gt;&lt;/li&gt;&lt;/ol&gt;&lt;/td&gt;&lt;td&gt;&lt;ol&gt;&lt;li&gt;&lt;font face=&quot;Courier New&quot;&gt;.cache()&lt;/font&gt;&lt;/li&gt;&lt;li&gt;&lt;font face=&quot;Courier New&quot;&gt;.persist()&lt;/font&gt;&lt;/li&gt;&lt;/ol&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;RDD&lt;br&gt;[org.apache.spark.rdd]&lt;/td&gt;&lt;td&gt;&lt;font face=&quot;Courier New&quot;&gt;val rdd = spark&lt;br&gt;&amp;nbsp; &amp;nbsp; .sparkContext&lt;br&gt;&amp;nbsp; &amp;nbsp; .textFile(&quot;people.txt&quot;)&lt;br&gt;&lt;br&gt;rdd.map(_.split(&quot;,&quot;))&lt;br&gt;&amp;nbsp; &amp;nbsp; .map(attrs =&amp;gt; Person(attrs(0),attrs(1).trim.toInt)&lt;br&gt;&lt;br&gt;&lt;br&gt;&lt;/font&gt;&lt;/td&gt;&lt;td&gt;&lt;ol&gt;&lt;li&gt;&lt;font face=&quot;Courier New&quot;&gt;map&lt;/font&gt;&lt;/li&gt;&lt;/ol&gt;&lt;/td&gt;&lt;td&gt;&lt;br&gt;&lt;/td&gt;&lt;td&gt;&lt;br&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;DateSet&lt;br&gt;[org.apache.spark.sql]&lt;/td&gt;&lt;td&gt;&lt;font face=&quot;Courier New&quot;&gt;case class Persion(name: String, age: Long)&lt;br&gt;&lt;br&gt;val ds = Seq(Person(&quot;Andy&quot;, 32)).toDS()&lt;br&gt;&lt;br&gt;// convert DataFrame to DataSet&lt;br&gt;df.as[Persion]&lt;br&gt;&lt;br&gt;// convert RDD to DataSet&lt;br&gt;rdd.toDS&lt;br&gt;&lt;br&gt;&lt;/font&gt;&lt;/td&gt;&lt;td&gt;&lt;br&gt;&lt;/td&gt;&lt;td&gt;&lt;br&gt;&lt;/td&gt;&lt;td&gt;&lt;br&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;DataFrame&lt;br&gt;[org.apache.spark.sql]&lt;br&gt;&lt;/td&gt;&lt;td&gt;&lt;div&gt;&lt;span style=&quot;font-family: &amp;#34;courier new&amp;#34;&quot;&gt;// this import is needed to use the $-notation&lt;/span&gt;&lt;br style=&quot;font-family: &amp;#34;courier new&amp;#34;&quot;&gt;&lt;span style=&quot;font-family: &amp;#34;courier new&amp;#34;&quot;&gt;import spark.implicits._&lt;/span&gt;&lt;font face=&quot;Courier New&quot;&gt;&lt;br&gt;&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-family: &amp;#34;courier new&amp;#34;&quot;&gt;&lt;br&gt;&lt;/span&gt;&lt;/div&gt;&lt;font face=&quot;Courier New&quot;&gt;val df = spark.read.textFile(&quot;hello.txt&quot;)&lt;br&gt;val df - spark.read.json(&quot;people.json&quot;)&lt;br&gt;&lt;br&gt;// convert RDD to DataFrame&lt;br&gt;&lt;/font&gt;&lt;span style=&quot;font-family: &amp;#34;courier new&amp;#34;&quot;&gt;rdd.toDF&lt;/span&gt;&lt;br&gt;&lt;/td&gt;&lt;td&gt;&lt;div&gt;&lt;ol&gt;&lt;li&gt;&lt;font face=&quot;Courier New&quot;&gt;df.select(&quot;name&quot;)&lt;/font&gt;&lt;/li&gt;&lt;li&gt;&lt;font face=&quot;Courier New&quot;&gt;df.select($&quot;name&quot;, $&quot;age&quot; + 1)&lt;/font&gt;&lt;/li&gt;&lt;li&gt;&lt;font face=&quot;Courier New&quot;&gt;df.filter($&quot;age&quot; &amp;gt; 21)&lt;/font&gt;&lt;/li&gt;&lt;li&gt;&lt;font face=&quot;Courier New&quot;&gt;df.groupBy(&quot;age&quot;).count()&lt;/font&gt;&lt;/li&gt;&lt;li&gt;&lt;font face=&quot;Courier New&quot;&gt;df.createOrReplaceTempView(&quot;people&quot;)&lt;/font&gt;&lt;/li&gt;&lt;li&gt;&lt;font face=&quot;Courier New&quot;&gt;df.createGlobalTempView(&quot;people&quot;)&lt;/font&gt;&lt;/li&gt;&lt;li&gt;&lt;font face=&quot;Courier New&quot;&gt;spark.sql(&quot;select * from people&quot;)&lt;/font&gt;&lt;/li&gt;&lt;li&gt;&lt;font face=&quot;Courier New&quot;&gt;spark.sql(&quot;select * from global_temp.people&quot;)&lt;/font&gt;&lt;/li&gt;&lt;li&gt;&lt;font face=&quot;Courier New&quot;&gt;spark.newSession().sql(&quot;select * from global_temp.people&quot;)&lt;/font&gt;&lt;/li&gt;&lt;/ol&gt;&lt;/div&gt;&lt;/td&gt;&lt;td&gt;&lt;ol&gt;&lt;li&gt;&lt;font face=&quot;Courier New&quot;&gt;.show()&lt;/font&gt;&lt;/li&gt;&lt;li&gt;&lt;span style=&quot;font-family: &amp;#34;courier new&amp;#34;&quot;&gt;.printScheme()&lt;/span&gt;&lt;/li&gt;&lt;/ol&gt;&lt;/td&gt;&lt;td&gt;&lt;br&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt; " style="verticalAlign=top;align=left;overflow=fill;fontSize=12;fontFamily=Helvetica;html=1;fillColor=none;" parent="Fq4y0ZjopHzJ_FTKpw_i-1" vertex="1">
