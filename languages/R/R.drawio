t face=&quot;courier new&quot;&gt;&amp;nbsp; &amp;nbsp; keep_tex: yes&lt;br&gt;&lt;/font&gt;&lt;span style=&quot;font-family: &amp;#34;courier new&amp;#34; ; font-size: 1em&quot;&gt;documentclass: book&lt;/span&gt;&lt;br&gt;&lt;span style=&quot;font-family: &amp;#34;courier new&amp;#34; ; font-size: 1em&quot;&gt;bibliography: [book.bib, packages.bib, ...]&lt;/span&gt;&lt;br&gt;&lt;span style=&quot;font-family: &amp;#34;courier new&amp;#34; ; font-size: 1em&quot;&gt;biblio-style: apalike&lt;/span&gt;&lt;br&gt;&lt;span style=&quot;font-family: &amp;#34;courier new&amp;#34; ; font-size: 1em&quot;&gt;link-citation: yes&lt;/span&gt;&lt;br&gt;&lt;span style=&quot;font-family: &amp;#34;courier new&amp;#34; ; font-size: 1em&quot;&gt;github-repo: rstudio/bookdown&lt;/span&gt;&lt;br&gt;&lt;span style=&quot;font-family: &amp;#34;courier new&amp;#34; ; font-size: 1em&quot;&gt;description: &quot;more info about the book&quot;&lt;/span&gt;&lt;br&gt;&lt;span style=&quot;font-family: &amp;#34;courier new&amp;#34; ; font-size: 1em&quot;&gt;---&lt;/span&gt;&lt;br&gt;&lt;h4&gt;2. Content&lt;/h4&gt;&lt;/div&gt;&lt;div&gt;&lt;ol&gt;&lt;li&gt;first level &quot;&lt;font face=&quot;Courier New&quot;&gt;#&lt;/font&gt;&quot; starts a new chapter&lt;/li&gt;&lt;/ol&gt;&lt;h3&gt;_bookdown.yml&lt;/h3&gt;&lt;/div&gt;&lt;div&gt;&lt;font face=&quot;Courier New&quot;&gt;book_filename: &quot;book name&quot;&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face=&quot;Courier New&quot;&gt;language:&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face=&quot;Courier New&quot;&gt;&amp;nbsp; ui:&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face=&quot;Courier New&quot;&gt;&amp;nbsp; &amp;nbsp; chapter_name: &quot;Chapter&quot;&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face=&quot;Courier New&quot;&gt;delete_merged_file: true&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face=&quot;Courier New&quot;&gt;rmd_files: [&quot;index.Rmd&quot;, &quot;abs.Rmd&quot;, &quot;intro.Rmd&quot;] #&amp;nbsp;&lt;/font&gt;define list and